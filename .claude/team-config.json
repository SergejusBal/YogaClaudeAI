{
  "team_name": "Full-Stack Diet & Fitness Development Team",
  "project_type": "Spring Boot + React Full Stack",
  "agents": [
    {
      "id": "backend-agent",
      "config_path": ".claude/agents/backend-agent.json",
      "primary_responsibilities": [
        "REST API development",
        "Database operations",
        "Security implementation",
        "External integrations"
      ]
    },
    {
      "id": "frontend-agent",
      "config_path": ".claude/agents/frontend-agent.json",
      "primary_responsibilities": [
        "React component development",
        "UI/UX implementation",
        "Client-side authentication",
        "Health calculators"
      ]
    },
    {
      "id": "database-agent",
      "config_path": ".claude/agents/database-agent.json",
      "primary_responsibilities": [
        "Database schema management",
        "Query optimization",
        "Caching strategies",
        "Data security"
      ]
    },
    {
      "id": "devops-agent",
      "config_path": ".claude/agents/devops-agent.json",
      "primary_responsibilities": [
        "CI/CD pipeline management",
        "Infrastructure provisioning",
        "Monitoring and logging",
        "Security hardening"
      ]
    },
    {
      "id": "testing-agent",
      "config_path": ".claude/agents/testing-agent.json",
      "primary_responsibilities": [
        "Test automation",
        "Quality assurance",
        "Performance testing",
        "Code coverage"
      ]
    },
    {
      "id": "security-agent",
      "config_path": ".claude/agents/security-agent.json",
      "primary_responsibilities": [
        "Security audits",
        "Vulnerability assessment",
        "Code review",
        "Compliance verification"
      ]
    },
    {
      "id": "design-agent",
      "config_path": ".claude/agents/design-agent.json",
      "primary_responsibilities": [
        "Visual design analysis",
        "UI/UX feedback and optimization",
        "Accessibility compliance testing",
        "Cross-device screenshot automation",
        "Design system consistency"
      ]
    }
  ],
  "mcp_servers": {
    "mysql-connector": ".claude/mcp-servers/mysql-connector.json",
    "maven-helper": ".claude/mcp-servers/maven-helper.json",
    "node-tools": ".claude/mcp-servers/node-tools.json",
    "redis-tools": ".claude/mcp-servers/redis-tools.json",
    "security-scanner": ".claude/mcp-servers/security-scanner.json",
    "deployment-helper": ".claude/mcp-servers/deployment-helper.json",
    "junit-runner": ".claude/mcp-servers/junit-runner.json",
    "vite-helper": ".claude/mcp-servers/vite-helper.json",
    "playwright-visual": ".claude/mcp-servers/playwright-visual.json",
    "design-analyzer": ".claude/mcp-servers/design-analyzer.json",
    "accessibility-checker": ".claude/mcp-servers/accessibility-checker.json"
  },
  "collaboration_rules": [
    "Backend and Database agents collaborate on data access patterns",
    "Frontend and Backend agents coordinate on API contracts",
    "DevOps agent handles deployment for all components",
    "Testing agent validates work from all other agents",
    "Security agent reviews code from all development agents",
    "Design agent provides UI/UX feedback to Frontend agent",
    "Design agent collaborates with Testing agent on accessibility compliance",
    "Design agent validates visual consistency across all user-facing components"
  ],
  "environment_variables": {
    "required": [
      "MYSQL_USER",
      "MYSQL_PASSWORD",
      "REDIS_PASSWORD",
      "JWT_SECRET",
      "OPENAI_API_KEY",
      "STRIPE_SECRET_KEY",
      "SENDGRID_API_KEY"
    ],
    "optional": [
      "SNYK_TOKEN",
      "GITHUB_TOKEN"
    ]
  }
}